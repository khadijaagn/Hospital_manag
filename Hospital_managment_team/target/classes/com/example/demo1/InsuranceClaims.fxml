<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml"  fx:controller="com.example.demo1.Controller.InsuranceClaimsController" spacing="10">
    <LineChart fx:id="lineChart">
        <xAxis>
            <CategoryAxis fx:id="yearAxis" label="Evolution Over the Years" />
        </xAxis>
        <yAxis>
            <NumberAxis fx:id="claimsAxis" label="Number of Claims" />
        </yAxis>
    </LineChart>
</VBox>
